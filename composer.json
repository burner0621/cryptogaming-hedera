{
  "name": "financialplugins/stake",
  "description": "Stake iGaming platform",
  "keywords": [
    "laravel",
    "casino",
    "games"
  ],
  "authors": [
    {
      "name": "FinancialPlugins",
      "email": "info@financialplugins.com",
      "homepage": "https://financialplugins.com",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.3|^8.0",
    "ext-bcmath": "*",
    "ext-gmp": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-sodium": "*",
    "ext-zip": "*",
    "bacon/bacon-qr-code": "^2.0",
    "fideloper/proxy": "^4.0",
    "financialplugins/omnipay-cryptapi": "^1.0",
    "financialplugins/omnipay-skrill": "^1.0",
    "financialplugins/omnipay-trongrid": "^1.1",
    "fruitcake/laravel-cors": "^1.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "iexbase/tron-api": "5.0",
    "kornrunner/ethereum-offline-raw-tx": "^0.4.0",
    "kornrunner/keccak": "^1.1",
    "kornrunner/secp256k1": "0.2",
    "laravel/framework": "^8.0",
    "laravel/sanctum": "^2.0",
    "laravel/socialite": "^5.1.0",
    "laravel/tinker": "^2.0",
    "laravel/ui": "^3.0",
    "league/omnipay": "^3.1",
    "mercadopago/dx-php": "^2.5",
    "omnipay/paypal": "^3.0",
    "omnipay/stripe": "^3.1",
    "paytm/paytmchecksum": "^1.1",
    "pragmarx/google2fa": "^7.0",
    "predis/predis": "^1.1",
    "pusher/pusher-php-server": "^4.1",
    "simplito/elliptic-php": "^1.0",
    "socialiteproviders/coinbase": "^4.0",
    "socialiteproviders/yahoo": "^4.0",
    "stephenhill/base58": "^1.1",
    "symfony/yaml": "^5.4"
  },
  "require-dev": {
    "facade/ignition": "^2.3.6",
    "beyondcode/laravel-dump-server": "^1.0",
    "doctrine/dbal": "^2.9",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "platform-check": false

  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/Helpers/constants.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php artisan jwt:secret --force --ansi"
    ]
  }
}
